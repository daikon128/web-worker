{
  "name": "web-worker",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "install-children": "npm-run-all -s install:link-card install:app-shell",
    "install:link-card": "yarn --cwd ./link-card --frozen-lockfile",
    "install:app-shell": "yarn --cwd ./app-shell --frozen-lockfile",
    "start-all": "npm-run-all -p \"start:*\"",
    "start:link-card": "yarn --cwd ./link-card start",
    "stop-all": "npm-run-all -p \"stop:*\"",
    "stop:link-card": "yarn --cwd ./link-card stop",
    "module-e2e": "npm-run-all -p \"e2e:*\"",
    "e2e:link-card": "yarn --cwd ./link-card e2e",
    "audit-all": "npm-run-all -p \"audit:*\"",
    "audit:link-card": "yarn --cwd ./link-card audit",
    "audit:app-shell": "yarn --cwd ./app-shell audit"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daikon128/web-worker.git"
  },
  "author": "daikon128",
  "bugs": {
    "url": "https://github.com/daikon128/web-worker/issues"
  },
  "license": "ISC",
  "homepage": "https://github.com/daikon128/web-worker#readme",
  "dependencies": {},
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
