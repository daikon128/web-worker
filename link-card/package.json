{
  "name": "link-card",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 8001",
    "build": "vite build",
    "start": "npm-run-all -s build preview",
    "stop": "kill $(lsof -t -i:8001)",
    "e2e": "cypress run",
    "preview": "vite preview --port 8001",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@sveltejs/vite-plugin-svelte": "^1.1.0",
    "@tsconfig/svelte": "^3.0.0",
    "cypress": "^11.0.1",
    "svelte": "^3.52.0",
    "svelte-check": "^2.9.2",
    "svelte-preprocess": "^4.10.7",
    "tslib": "^2.4.0",
    "typescript": "^4.6.4",
    "vite": "^3.2.3"
  },
  "dependencies": {
    "npm-run-all": "^4.1.5"
  }
}
